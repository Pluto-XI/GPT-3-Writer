{
  "name": "GPT-3 Writer",
  "description": "Write song lyrics in the style of your favorite artists anywhere!",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "48": "assets/48.png",
    "72": "assets/72.png",
    "96": "assets/96.png",
    "144": "assets/144.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Generate song lyrics"
  },
  "permissions": ["contextMenus", "tabs", "storage"]
}
